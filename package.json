    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "scripts": {
    "start": "webpack-dev-server --port 8080",
    "lint": "yarn eslint:check && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "eslint:check": "eslint src",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:check": "prettier --check '*.js' 'src/**/*.{js,scss}'",
    "prettier:fix": "prettier --write '*.js' 'src/**/*.{js,scss}'",
    "dist:build": "NODE_ENV=production webpack --bail",
    "dist:serve": "static dist/",
    "dist:open": "yarn dist:build && yarn dist:serve",
    "deploy": "yarn dist:build && vercel --prod"
