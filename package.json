{
  "name": "fuel-calc-pwa",
  "license": "MIT",
  "version": "2.0.0",
  "dependencies": {
    "joi": "17.6.0",
    "preact": "10.8.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "2.3.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.18.0",
    "eslint-config-preact": "1.3.0",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-plugin-pwa": "0.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check . && eslint 'src/**/*.{js,jsx,ts,tsx,html}' && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write . && eslint 'src/**/*.{js,jsx,ts,tsx,html}' --fix"
  }
