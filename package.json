    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-inline-svg-plugin": "^2.2.6",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "modern-normalize": "^1.1.0",
    "node-sass": "^7.0.1",
