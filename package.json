{
  "name": "fuel-calc-pwa",
  "license": "MIT",
  "version": "2.0.0",
  "dependencies": {
    "joi": "17.10.1",
    "preact": "10.11.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "2.5.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-preact": "1.3.0",
    "postcss": "8.4.29",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "vite": "3.2.7",
    "vite-plugin-pwa": "0.13.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check . && eslint 'src/**/*.{js,jsx,ts,tsx,html}' && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write . && eslint 'src/**/*.{js,jsx,ts,tsx,html}' --fix"
  }
