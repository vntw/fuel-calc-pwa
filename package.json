    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-inline-svg-plugin": "^2.2.6",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "modern-normalize": "^1.1.0",
    "node-sass": "^6.0.1",
